<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragmentSomeDetail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SomeDetailFragment">


    <Button
        android:id="@+id/button_to_home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="141dp"
        android:layout_marginTop="151dp"
        android:layout_marginEnd="142dp"
        android:layout_marginBottom="66dp"
        android:text="Go to Home"
        app:layout_constraintBottom_toTopOf="@+id/button_to_market"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_to_market"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginTop="66dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="418dp"
        android:text="Go to Market"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_to_home" />
</androidx.constraintlayout.widget.ConstraintLayout>